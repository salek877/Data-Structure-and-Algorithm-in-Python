    "def palindrome(s):\n",
    "    if s == s[::-1]:\n",
    "        return True\n",
    "    \n",
    "    return False\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    print(palindrome('madam'))\n",
    "    print(palindrome('hello'))"
